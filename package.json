    "start": "start cmd.exe /C gulp codeblocks && start cmd.exe /C npm run watch",
    "build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --aot --app=debug",
    "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --app=debug",
    "watch": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --aot --app=debug",
    "test": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test",
    "test-tc": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --config ./test/karma-teamcity.conf.js --code-coverage",
    "lint": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng lint",
    "e2e": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng e2e",
    "clean": "gulp cleanblocks",
    "ng-build-lib": "start cmd.exe /C gulp",
    "ng-pack": "start cmd.exe /C gulp npm-pack"
